        &lt;span class="nv"&gt;directions&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; bus.findtext&lt;span class="o"&gt;(&lt;/span&gt;&lt;span class="s1"&gt;&amp;#39;d&amp;#39;&lt;/span&gt;&lt;span class="o"&gt;)&lt;/span&gt;
        &lt;span class="k"&gt;if&lt;/span&gt; directions.startswith&lt;span class="o"&gt;(&lt;/span&gt;&lt;span class="s1"&gt;&amp;#39;North&amp;#39;&lt;/span&gt;&lt;span class="o"&gt;)&lt;/span&gt;:
            &lt;span class="nv"&gt;busid&lt;/span&gt; &lt;span class="o"&gt;=&lt;/span&gt; bus.findtext&lt;span class="o"&gt;(&lt;/span&gt;&lt;span class="s1"&gt;&amp;#39;id&amp;#39;&lt;/span&gt;&lt;span class="o"&gt;)&lt;/span&gt;
